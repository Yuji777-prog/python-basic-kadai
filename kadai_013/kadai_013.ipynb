   "source": [
    "# 関数の定義\n",
    "def calc_total(price, zei):\n",
    "  #与えられた引数より合計金額を計算する\n",
    "  total = price * (1 + zei / 100)\n",
    "\n",
    "  #計算結果を返す\n",
    "  return total\n",
    "\n",
    "\n",
    "# 商品の金額「110」,消費税「10」%を引数として関数を実行し、戻り値を出力する\n",
    "print(calc_total(110,10))"
   ]
  }
